{:paths ["src" "resources"]

 :deps
 {blaze/db
  {:local/root "../db"}

  com.fasterxml.jackson.module/jackson-module-jaxb-annotations
  {:mvn/version "2.13.3"}

  info.cqframework/cql-to-elm
  {:mvn/version "1.5.9"
   :exclusions
   [com.google.code.javaparser/javaparser
    info.cqframework/qdm
    junit/junit
    xpp3/xpp3
    xpp3/xpp3_xpath
    org.slf4j/jcl-over-slf4j
    org.slf4j/slf4j-api
    org.jvnet.jaxb2_commons/jaxb2-basics-tools]}

  javax.xml.bind/jaxb-api
  {:mvn/version "2.4.0-b180830.0359"}

  systems.uom/systems-quantity
  {:mvn/version "2.1"}

  systems.uom/systems-ucum
  {:mvn/version "2.1"}}

 :aliases
 {:test
  {:extra-paths ["test"]

   :extra-deps
   {blaze/db-stub
    {:local/root "../db-stub"}

    org.clojure/data.xml
    {:mvn/version "0.2.0-alpha6"}}}

  :kaocha
  {:extra-deps
   {lambdaisland/kaocha
    {:mvn/version "1.68.1059"}}

   :main-opts ["-m" "kaocha.runner"]}

  :coverage
  {:extra-deps
   {cloverage/cloverage
    {:mvn/version "1.2.4"}}

   :main-opts ["-m" "cloverage.coverage" "--codecov" "-p" "src" "-s" "test"
               "-e" ".*spec$"]}}}
