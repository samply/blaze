{:paths ["src"]

 :deps
 {blaze/db
  {:local/root "../db"}

  com.fasterxml.jackson.module/jackson-module-jaxb-annotations
  {:mvn/version "2.17.0"}

  com.fasterxml.jackson.dataformat/jackson-dataformat-xml
  {:mvn/version "2.17.0"}

  info.cqframework/cql-to-elm
  {:mvn/version "3.10.0"
   :exclusions
   [org.antlr/antlr4
    xpp3/xpp3
    xpp3/xpp3_xpath]}

  info.cqframework/elm-jackson
  {:mvn/version "3.10.0"
   :exclusions [org.glassfish/javax.json]}

  info.cqframework/model-jackson
  {:mvn/version "3.10.0"}

  info.cqframework/quick
  {:mvn/version "3.10.0"}

  org.apache.commons/commons-text
  {:mvn/version "1.12.0"}

  systems.uom/systems-quantity
  {:mvn/version "2.1"}

  systems.uom/systems-ucum
  {:mvn/version "2.1"}}

 :aliases
 {:test
  {:extra-paths ["test"]

   :extra-deps
   {blaze/db-stub
    {:local/root "../db-stub"}}}

  :test-perf
  {:extra-paths ["test-perf"]

   :extra-deps
   {criterium/criterium
    {:mvn/version "0.4.6"}}}

  :kaocha
  {:extra-deps
   {lambdaisland/kaocha
    {:mvn/version "1.88.1376"}}

   :main-opts ["-m" "kaocha.runner"]}

  :coverage
  {:extra-deps
   {cloverage/cloverage
    {:mvn/version "1.2.4"}}

   :main-opts ["-m" "cloverage.coverage" "--codecov" "-p" "src" "-s" "test"
               "-e" ".*spec$"]}}}
